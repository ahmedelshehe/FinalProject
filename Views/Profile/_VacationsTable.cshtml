@model List<Vacation>
<table class="table table-striped table-hover ">
	<thead>
		<tr>
			<th>Start Date</th>
			<th>End Date</th>
			<th>Day/s</th>
			<th>Status</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var vacation in Model)
		{
			<tr>
				<td>@vacation.StartDate?.ToString("d MMM yyyy")</td>
				<td>@vacation.EndDate?.ToString("d MMM yyyy")</td>
				<td>@vacation.VacationDays</td>
				<td>@vacation.Status</td>
			</tr>
		}
	</tbody>
</table>
